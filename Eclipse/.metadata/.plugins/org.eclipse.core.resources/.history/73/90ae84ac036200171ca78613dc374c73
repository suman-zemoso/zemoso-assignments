package subwayRecipes;

import java.util.List;

public abstract class SubBuilder {

	private Sub sub;
	
	public abstract void addBread(Bread bread);
	
	public abstract void addSalad(Salad salad);
	
	public abstract void addSauce(List<Sauce> sauce);
	
	public void addAddOns(List<AddOn> addOns){
		
	}
}
