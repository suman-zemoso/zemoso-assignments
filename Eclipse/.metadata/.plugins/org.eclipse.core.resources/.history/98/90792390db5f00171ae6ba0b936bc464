package utooCabService;

public class Main {

	public static void main(String[] args) {

		BranchFactory bf1 = new HydBranchFactory();
		BranchFactory bf2 = new BanBranchFactory();
		BranchFactory bf3 = new ChenBranchFactory();
		Cab c1 = BanBranchFactory.makeCab("Mini");
		Cab c2 = BanBranchFactory.makeCab("Micro");
		Cab c3 = BanBranchFactory.makeCab("SUV");
		Cab c4 = BanBranchFactory.makeCab("Sedan");
		Cab c5 = HydBranchFactory.makeCab("Mini");
		Cab c6 = HydBranchFactory.makeCab("Micro");
		Cab c7 = HydBranchFactory.makeCab("SUV");
		Cab c8 = ChenBranchFactory.makeCab("Mini");
		Cab c9 = ChenBranchFactory.makeCab("Micro");
		Cab c10 = ChenBranchFactory.makeCab("Sedan");
		System.out.println("Rate Charts of Cabs in Bangalore: ");
		c1.printRateChart();
		c2.printRateChart();
		c3.printRateChart();
		c4.printRateChart();
		System.out.println("Rate Charts of Cabs in Hyderabad: ");
		c5.printRateChart();
		c6.printRateChart();
		c7.printRateChart();
		System.out.println("Rate Charts of Cabs in Chennai: ");
		c8.printRateChart();
		c9.printRateChart();
		c10.printRateChart();
		

	}

}
