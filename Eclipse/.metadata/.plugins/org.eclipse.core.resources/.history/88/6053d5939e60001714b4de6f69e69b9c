package alphabets;

import java.util.Scanner;

public class Alphabets {
	private boolean containsAllLetters(String letters){
		if(letters.length() < 26)
			return false;
		for()
		
		
		return false;
	}

	public static void main(String[] args) {

		Alphabets alphabets = new Alphabets();
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the sequence.");
		boolean allAlphabetsCheck = alphabets.containsAllLetters(sc.nextLine());//b is boolean that stores true if input contains all the alphabets otherwise false.
		System.out.println("your input " + (allAlphabetsCheck?"":"does not") + "contains all the alphabets.");//here b edits the line to be printed according to it's value.
		sc.close();
	}

}
