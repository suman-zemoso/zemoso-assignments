package utooCabService;

public class FactoryBuilder {
	public BranchFactory makeFactory(String city){
		BranchFactory bf;
		if(city.equals("Hyd")){bf = new HydBranchFactory();}
		else if(city.equals("Ban")){bf = new BanBranchFactory();}
		else if(city.equals("Chen")){bf = new ChenBranchFactory();}
	}
}
